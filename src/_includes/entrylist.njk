  <section class="kb-section">
    <h1 class="kb-section-header">{{entrylistHeader}}</h1>
    <div class="kb-section-entry-container">
    {% for entry in entrylist | reverse %}
      <div class="kb-section-entry{% if entryCollection.indexOf(entry) == entryPrimary %} primary{% endif %}">
        <a href="{{ entry.url | url }}"><h2 class="kb-section-entry-title"><span>{% if entry.data.title %}{{ entry.data.title }}{% else %}<code>{{ entry.url }}</code>{% endif %}</span></h2></a>
          <p><time class="postlist-date" datetime="{{ entry.date | htmlDateString }}">{{ entry.date | readableDate }}</time></p>
          <p>{{ entry.data.description }}</p>
          <p>
          {% for tag in entry.data.tags %}
            {%- if collections.tagList.indexOf(tag) != -1 -%}
            {% set tagUrl %}/tags/{{ tag }}/{% endset %}
            <a href="{{ tagUrl | url }}" class="tag">{{ tag }}</a>
            {%- endif -%}
          {% endfor %}
          </p>
      </div>
    {% endfor %}
    </div>
  </section>
